<h2 class="center-align">Find the Perfect College</h2>
    
    <div class="container">
    
    <h2 class="center-align">College Match</h2>
    
        <p>User Search</p>
    
        <form class="col s12">
            <div class="row">
            <div class="input-field col s6">
                <input id="first_name" type="text" class="validate">
                <label for="first_name">First Name</label>
            </div>
            <div class="input-field col s6">
                <input id="last_name" type="text" class="validate">
                <label for="last_name">Last Name</label>
            </div>
            </div>
    
        <div class="row">
            <div class="input-field col s12">
                <input id="email" type="text" class="validate">
                <label for="email">Email</label>
            </div>
        </div>
    
        <div class="row">
        <label>Degree Type</label>
        <select class="browser-default">
            <option value="" disabled selected>Choose One:</option>
            <option value="1">2 Year Degree (Associates)</option>
            <option value="2">4 Year Degree (Bachelors)</option>
        </select>
        </div>
    
        <div class="row">
            <label>Home State</label>
            <select class="browser-default">
                <option value="" disabled selected>Choose One:</option>
                <option value="1">North Carolina</option>
                <option value="2">Other</option>
            </select>
        </div>
    
        <div class="row">
            <label>Program</label>
            <select class="browser-default">
                <option value="" disabled selected>Choose One:</option>
                <option value="1">Science</option>
                <option value="2">Arts</option>
            </select>
        </div>
    
        <div class="row">
    
            <div class="input-field col s6">
                <input id="price" type="text" class="validate">
                <label for="price">Maximum Tuition ($)</label>
            </div>
            <!-- <label>Maximum Tuition</label>
            <p class="range-field">
                <input type="range" id="test5" min="0" max="50000" />
            </p> -->
        </div>
        
        <div class="row">
            <button class="btn waves-effect waves-light blue" type="submit" name="action">Submit
            </button>
            <button class="btn waves-effect waves-light" type="submit" name="action">Guest Search
            </button>
        </div>
        </form>
        
    </div>